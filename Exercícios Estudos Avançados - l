#include <stdio.h>
int main()

{
    float cotacao_dolar, quantidade_real, valor_conversao;

    printf("Informe a cotação do dólar (em reais por dólar): ");
    scanf("%f", &cotacao_dolar);

    printf("Informe a quantidade de reais: ");
    scanf("%f", &quantidade_real);

    valor_conversao = quantidade_real / cotacao_dolar;

    printf("O valor em dólares é: US$ %.2f\n", valor_conversao);

    return 0;
}
